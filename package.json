    "url": "https://github.com/rhardih/winski/issues"
  },
  "homepage": "https://github.com/rhardih/winski#readme",
  "dependencies": {
    "ampersand-state": "^5.0.2",
    "ampersand-view": "^10.0.1",
    "canvg-browser": "^1.0.0",
    "file-saver": "^1.3.2",
    "handlebars": "^4.0.5",
    "hbsfy": "^2.7.0",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "bundle-collapser": "^1.2.1",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
